{
  "expo": {
    "name": "She Matters",
    "slug": "she-matters",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon-shematters.png",
    "scheme": "com.remedypointsolutions.shematters",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/images/splash-shematters.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "usesAppleSignIn": true,
      "bundleIdentifier": "com.remedypointsolutions.shematters"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-apple-authentication"],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    }
  }
}
